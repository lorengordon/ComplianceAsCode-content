<def-group>
  <definition class="compliance" id="package_audit-audispd-plugins_installed"
  version="1">
    <metadata>
        <title>Ensure the default plugins for the audit dispatcher are Installed</title>
            <affected family="unix">
                <platform>multi_platform_all</platform>
            </affected>
        <description>The DPKG package audit-audispd-plugins should be installed.</description>
    </metadata>
    <criteria>
      <criterion comment="package audit-audispd-plugins is installed"
      test_ref="test_package_audit-audispd-plugins_installed" />
    </criteria>
  </definition>

  <linux:dpkginfo_test check="all" check_existence="all_exist"
  id="test_package_audit-audispd-plugins_installed" version="1"
  comment="package audit-audispd-plugins is installed">
    <linux:object object_ref="obj_test_package_audit-audispd-plugins_installed" />
    
  </linux:dpkginfo_test>
  <linux:dpkginfo_object id="obj_test_package_audit-audispd-plugins_installed" version="1">
    <linux:name>audit-audispd-plugins</linux:name>
  </linux:dpkginfo_object>
  

</def-group>
<def-group>
  <definition class="compliance" id="package_systemd-journal-remote_installed"
  version="1">
    <metadata>
        <title>Install systemd-journal-remote Package</title>
            <affected family="unix">
                <platform>multi_platform_all</platform>
            </affected>
        <description>The RPM package systemd-journal-remote should be installed.</description>
    </metadata>
    <criteria>
      <criterion comment="package systemd-journal-remote is installed"
      test_ref="test_package_systemd-journal-remote_installed" />
    </criteria>
  </definition>

  <linux:rpminfo_test check="all" check_existence="all_exist"
  id="test_package_systemd-journal-remote_installed" version="1"
  comment="package systemd-journal-remote is installed">
    <linux:object object_ref="obj_test_package_systemd-journal-remote_installed" />
    
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_test_package_systemd-journal-remote_installed" version="1">
    <linux:name>systemd-journal-remote</linux:name>
  </linux:rpminfo_object>
  

</def-group>